{
  "version": 2,
  "routes": [
    {
      "src": "^/api/(.*)",
      "dest": "/api/server.js"
    },
    {
      "src": "^/$",
      "dest": "/public/index.html"
    },
    {
      "src": "^/home$",
      "dest": "/public/home.html"
    },
    {
      "src": "^/signup$",
      "dest": "/public/signup.html"
    },
    {
      "src": "^/profile$",
      "dest": "/public/profile.html"
    },
    {
      "src": "^/admin$",
      "dest": "/public/admin.html"
    },
    {
      "src": "^/admin-login$",
      "dest": "/public/admin-login.html"
    },
    {
      "src": "^/(.*)$",
      "dest": "/public/$1"
    }
  ]
}
